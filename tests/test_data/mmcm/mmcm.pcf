# basys3 100 MHz CLK
set_io clk W5

# sw[7:0] correspond with SW7-SW0 on the basys3
set_io sw[0] V17
set_io sw[1] V16
set_io sw[2] W16
set_io sw[3] W17
set_io sw[4] W15
set_io sw[5] V15
set_io sw[6] W14
set_io sw[7] W13

# led[7:0] correspond with LD7-LD0 on the basys3
set_io led[0] U16
set_io led[1] E19
set_io led[2] U19
set_io led[3] V19
set_io led[4] W18
set_io led[5] U15
set_io led[6] U14
set_io led[7] V14

# PMOD JC
set_io jc1 K17
set_io jc2 M18
set_io jc3 N17
set_io jc4 L17
